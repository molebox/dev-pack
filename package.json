{
  "name": "dev-pack",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/molebox/dev-pack.git",
  "author": "Richard Haines <hello@richardhaines.dev>",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "pretty-check": "prettier --check \"src/**/*.{js}\"",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "sitemap": "gatsby build && gatsby serve",
    "z": "gatsby clean && gatsby develop",
    "net": "gatsby clean && netlify dev"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "proseWrap": "always"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.9",
    "@apollo/link-context": "^2.0.0-beta.3",
    "cloudinary": "^1.22.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.3",
    "gatsby-plugin-emotion": "^4.3.4",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-theme-seo": "^1.1.1",
    "global": "^4.4.0",
    "graphql": "^15.1.0",
    "gsap": "^3.3.3",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "netlify-cli": "^2.58.0",
    "netlify-lambda": "^1.6.3",
    "onegraph-apollo-client": "^2.2.2",
    "onegraph-auth": "^2.2.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.2",
    "react-loadable": "^5.5.0",
    "react-loader-spinner": "^3.1.14",
    "react-tabs": "^3.1.1",
    "react-toastify": "^6.0.8",
    "reactour": "^1.18.0",
    "styled-components": "^4.0.0",
    "theme-ui": "^0.3.1",
    "use-cloudinary": "^3.1.6",
    "windups": "^1.0.2"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "prettier": "^2.0.5"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
